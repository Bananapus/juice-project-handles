  "scripts": {
    "chain": "hardhat node",
    "account": "hardhat account",
    "test": "mocha './test/**/*.test.js' -r esm --bail",
    "coverage": "node --require esm ./node_modules/.bin/hardhat coverage --network hardhat",
    "clean": "rimraf ./cache && rimraf ./artifacts",
    "compile": "yarn clean && hardhat compile",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,sol}\"",
    "deploy-rinkeby": "source .env && forge script DeployRinkeby --rpc-url $RINKEBY_RPC_PROVIDER_URL --broadcast --private-key $PRIVATE_KEY --sender $SENDER_ADDRESS --verify --etherscan-api-key $ETHERSCAN_API_KEY",
    "deploy-mainnet": "source .env && forge script DeployMainnet --rpc-url $MAINNET_RPC_PROVIDER_URL --broadcast --private-key $PRIVATE_KEY --sender $SENDER_ADDRESS --verify --etherscan-api-key $ETHERSCAN_API_KEY"
  },
  "dependencies": {}
